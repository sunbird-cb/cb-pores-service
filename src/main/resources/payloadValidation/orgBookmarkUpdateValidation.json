{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "title": {
      "type": "string"
    },
    "orgBookmarkId": {
      "type": "string"
    },
    "category": {
      "type": "string"
    },
    "imageUrl": {
      "type": "string",
      "format": "uri"
    },
    "orgId": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "orgList": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "minItems": 1
    }
  },
  "required": [
    "category",
    "orgList",
    "orgId",
    "orgBookmarkId"
  ]
}